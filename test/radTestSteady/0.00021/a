FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0.00021";
    object      a;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    leftWall
    {
        type            calculated;
        value           uniform 1;
    }
    rightWall
    {
        type            calculated;
        value           uniform 1;
    }
    empty
    {
        type            empty;
    }
}


