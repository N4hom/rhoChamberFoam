FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "4e-07";
    object      qr;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    leftWall
    {
        type            calculated;
        value           uniform -1.12749e+07;
    }
    rightWall
    {
        type            calculated;
        value           uniform 310782;
    }
    empty
    {
        type            empty;
    }
}


